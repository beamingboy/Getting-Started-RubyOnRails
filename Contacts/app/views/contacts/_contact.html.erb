<tr id="<%= dom_id contact %>">
  <td>
    <%= contact.first_name %>
  </td>

  <td>
    <%= contact.last_name %>
  </td>

  <td>
    <%= contact.phone %>
  </td>

  <td>
    <%= contact.email %>
  </td>
  <td>
    <button type="button" class="btn btn-secondary">
    <%= link_to "Show",contact, class:"text-white" %>
    </button>
  </td>
  <td>
    <button type="button" class="btn btn-secondary">
    <%= link_to "Edit",edit_contact_path(contact), class:"text-white" %>
    </button>
  </td>
  <td>
    <button type="button" class="btn btn-danger text-white">
      <%= link_to 'Destroy',contact, :method => :delete,  :confirm => 'Are you sure?', class:"text-white"%>
    </button>
  </td>

</tr>
