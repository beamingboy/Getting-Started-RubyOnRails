<tr id="<%= dom_id contact %>">
  <td>
    <%= contact.first_name %>
  </td>

  <td>
    <%= contact.last_name %>
  </td>

  <td>
    <%= contact.phone %>
  </td>

  <td>
    <%= contact.email %>
  </td>
  <td>
    <%= link_to "Show",contact, class:"text-white btn btn-secondary" %>
  </td>
  <td>
    <%= link_to "Edit",edit_contact_path(contact), class:"text-white btn btn-secondary" %>
  </td>
  <td>
      <%= button_to "Delete", contact, method: :delete,class:"btn btn-danger text-white" %>
  </td>

</tr>
